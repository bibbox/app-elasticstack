{
    "makefolders":[
        "data",
        "var/config/logstash",
        "var/config/kibana"
    ],
    "copyfiles":[
        {
            "source":"config/logstash/logstash.conf",
            "destination":"var/config/logstash/logstash.conf"
        },
        {
            "source":"config/kibana/kibana.yml",
            "destination":"var/config/kibana/kibana.yml"
        }
    ]
}